{
    "name": "rekalogika/rekapager",
    "description": "Pagination library for PHP, supporting both offset-based and keyset-based pagination.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "homepage": "https://rekalogika.dev/rekapager",
    "require": {
        "php": "^8.2",
        "composer-runtime-api": "^2.0",
        "api-platform/core": "^3.2 || ^4.0",
        "doctrine/collections": "^2.2",
        "doctrine/dbal": "^3.8 || ^4.0",
        "doctrine/orm": "^2.19 || ^3.0",
        "pagerfanta/core": "^4.0",
        "pagerfanta/doctrine-collections-adapter": "^4.0",
        "psr/container": "^2.0",
        "spomky-labs/base64url": "^2.0",
        "symfony/config": "^6.4 || ^7.0 || ^8.0",
        "symfony/dependency-injection": "^6.4 || ^7.0 || ^8.0",
        "symfony/http-foundation": "^6.4 || ^7.0 || ^8.0",
        "symfony/http-kernel": "^6.4 || ^7.0 || ^8.0",
        "symfony/polyfill-intl-messageformatter": "^1.27",
        "symfony/routing": "^6.4 || ^7.0 || ^8.0",
        "symfony/serializer": "^6.4 || ^7.0 || ^8.0",
        "symfony/stimulus-bundle": "^2.16",
        "symfony/translation": "^6.4 || ^7.0 || ^8.0",
        "symfony/web-link": "^6.4 || ^7.0 || ^8.0",
        "twig/twig": "^3.8"
    },
    "require-dev": {
        "bnf/phpstan-psr-container": "^1.0",
        "composer/semver": "^3.4",
        "doctrine/doctrine-bundle": "^2.11.3 || ^3.0",
        "doctrine/doctrine-fixtures-bundle": "^3.5 || ^4.0",
        "doctrine/persistence": "^3.1",
        "ekino/phpstan-banned-code": "^3.0",
        "fakerphp/faker": "^1.23",
        "graviton/link-header-rel-parser": "^1.0",
        "phpat/phpat": "^0.11 || ^0.12",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^10.5",
        "psr/log": "^2.0 || ^3.0",
        "ramsey/uuid": "^4.7",
        "rector/rector": "^2.0",
        "rector/swiss-knife": "^2.2",
        "rekalogika/doctrine-collections-decorator": "^2.3",
        "symfony/asset": "^6.4 || ^7.0 || ^8.0",
        "symfony/asset-mapper": "^6.4 || ^7.0 || ^8.0",
        "symfony/browser-kit": "^6.4 || ^7.0 || ^8.0",
        "symfony/debug-bundle": "^6.4 || ^7.0 || ^8.0",
        "symfony/doctrine-bridge": "^6.4 || ^7.0 || ^8.0",
        "symfony/dotenv": "^6.4 || ^7.0 || ^8.0",
        "symfony/error-handler": "^6.4 || ^7.0 || ^8.0",
        "symfony/form": "^6.4 || ^7.0 || ^8.0",
        "symfony/framework-bundle": "^6.4 || ^7.0 || ^8.0",
        "symfony/http-client": "^6.4 || ^7.0 || ^8.0",
        "symfony/maker-bundle": "^1.55",
        "symfony/monolog-bundle": "^3.0",
        "symfony/runtime": "^6.4 || ^7.0 || ^8.0",
        "symfony/twig-bundle": "^6.4 || ^7.0 || ^8.0",
        "symfony/uid": "^6.4 || ^7.0 || ^8.0",
        "symfony/validator": "^6.3 || ^7.0 || ^8.0",
        "symfony/var-dumper": "^6.3 || ^7.0 || ^8.0",
        "symfony/web-profiler-bundle": "^6.4 || ^7.0 || ^8.0",
        "symfony/yaml": "^6.4 || ^7.0 || ^8.0",
        "symplify/monorepo-builder": "^11.2.20 || ^11.3",
        "zenstruck/foundry": "^2.8"
    },
    "replace": {
        "rekalogika/rekapager-adapter-common": "1.0.4",
        "rekalogika/rekapager-api-platform": "1.0.4",
        "rekalogika/rekapager-bundle": "1.0.4",
        "rekalogika/rekapager-contracts": "1.0.4",
        "rekalogika/rekapager-core": "1.0.4",
        "rekalogika/rekapager-doctrine-collections-adapter": "1.0.4",
        "rekalogika/rekapager-doctrine-dbal-adapter": "1.0.4",
        "rekalogika/rekapager-doctrine-orm-adapter": "1.0.4",
        "rekalogika/rekapager-keyset-pagination": "1.0.4",
        "rekalogika/rekapager-offset-pagination": "1.0.4",
        "rekalogika/rekapager-pagerfanta-adapter": "1.0.4",
        "rekalogika/rekapager-symfony-bridge": "1.0.4"
    },
    "autoload": {
        "psr-4": {
            "Rekalogika\\Contracts\\Rekapager\\": "packages/rekapager-contracts/src/",
            "Rekalogika\\Rekapager\\": "packages/rekapager-core/src/",
            "Rekalogika\\Rekapager\\Adapter\\Common\\": "packages/rekapager-adapter-common/src/",
            "Rekalogika\\Rekapager\\ApiPlatform\\": "packages/rekapager-api-platform/src/",
            "Rekalogika\\Rekapager\\Bundle\\": "packages/rekapager-bundle/src/",
            "Rekalogika\\Rekapager\\Doctrine\\Collections\\": "packages/rekapager-doctrine-collections-adapter/src/",
            "Rekalogika\\Rekapager\\Doctrine\\DBAL\\": "packages/rekapager-doctrine-dbal-adapter/src/",
            "Rekalogika\\Rekapager\\Doctrine\\ORM\\": "packages/rekapager-doctrine-orm-adapter/src/",
            "Rekalogika\\Rekapager\\Keyset\\": "packages/rekapager-keyset-pagination/src/",
            "Rekalogika\\Rekapager\\Offset\\": "packages/rekapager-offset-pagination/src/",
            "Rekalogika\\Rekapager\\Pagerfanta\\": "packages/rekapager-pagerfanta-adapter/src/",
            "Rekalogika\\Rekapager\\Symfony\\": "packages/rekapager-symfony-bridge/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rekalogika\\Rekapager\\Tests\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
